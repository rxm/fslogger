ce52fdb7e4c1"],

Cell[BoxData[
 RowBox[{
  RowBox[{"rndThing", "[", "level_", "]"}], ":=", 
  RowBox[{"If", "[", 
   RowBox[{
    RowBox[{
     RowBox[{"level", "==", "0"}], " ", "\[Or]", " ", 
     RowBox[{
      RowBox[{"RandomReal", "[", "]"}], ">", 
      RowBox[{"1", "/", "5"}]}]}], ",", " ", "\[IndentingNewLine]", 
    RowBox[{"RandomChoice", "[", " ", 
     RowBox[{"{", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{"RandomInteger", "[", 
          RowBox[{"{", 
           RowBox[{"1", ",", "15"}], "}"}], "]"}], ",", " ", 
         RowBox[{"RandomInteger", "[", 
          RowBox[{"{", 
           RowBox[{"4", ",", "8"}], "}"}], "]"}]}], "]"}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{"rndSent", "[", "]"}], ",", "\[IndentingNewLine]", 
       RowBox[{"RandomInteger", "[", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"2", "^", "10"}], ",", 
          RowBox[{"2", "^", "15"}]}], "}"}], "]"}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{"RandomReal", "[", 
        RowBox[{"{", 
         RowBox[{"0", ",", "2"}], "}"}], "]"}]}], "\[IndentingNewLine]", 
      "}"}], "]"}], ",", "\[IndentingNewLine]", "\[IndentingNewLine]", 
    RowBox[{"(*", " ", "else", " ", "*)"}], "\[IndentingNewLine]", 
    RowBox[{"<|", 
     RowBox[{
      RowBox[{
       RowBox[{"RandomWord", "[", "]"}], "\[Rule]", 
       RowBox[{"rndThing", "[", "0", "]"}]}], ",", " ", 
      RowBox[{
       RowBox[{"RandomWord", "[", "]"}], "\[Rule]", " ", 
       RowBox[{"rndThing", "[", 
        RowBox[{"level", "-", "1"}], "]"}]}]}], "|>"}]}], 
   "\[IndentingNewLine]", "]"}]}]], "Input",
 CellChangeTimes->{{3.7239752637912083`*^9, 3.723975355995343*^9}, {
