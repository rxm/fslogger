5fba6739502c"],

Cell[BoxData[
 RowBox[{
  RowBox[{"rndAssociation", "[", 
   RowBox[{"depth_", ",", "n_"}], "]"}], ":=", 
  RowBox[{"Module", "[", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"{", "k", "}"}], ",", "\[IndentingNewLine]", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"k", "=", 
      RowBox[{"RandomSample", "[", 
       RowBox[{
        RowBox[{"WordList", "[", "\"\<Noun\>\"", "]"}], ",", "n"}], "]"}]}], 
     ";", "\[IndentingNewLine]", 
     RowBox[{"<|", 
      RowBox[{"Thread", "[", 
       RowBox[{"k", "\[Rule]", 
        RowBox[{"Table", "[", 
         RowBox[{
          RowBox[{"rndThing", "[", "depth", "]"}], ",", "n"}], "]"}]}], "]"}],
       "|>"}]}]}], "\[IndentingNewLine]", "]"}]}]], "Input",
