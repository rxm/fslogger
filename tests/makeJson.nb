Cell[BoxData[
 RowBox[{
  RowBox[{"rndSent", "[", "]"}], ":=", " ", 
  RowBox[{"StringRiffle", "[", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"Table", "[", 
     RowBox[{
      RowBox[{"RandomWord", "[", "]"}], ",", " ", 
      RowBox[{"RandomInteger", "[", 
       RowBox[{"{", 
        RowBox[{"3", ",", "9"}], "}"}], "]"}]}], "]"}], ",", 
    "\[IndentingNewLine]", "\"\< \>\""}], "\[IndentingNewLine]", 
